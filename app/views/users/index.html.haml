- model_class = User
.row.grid
  .col-md-12
    = flash_message
    .panel.panel-piluku
      .panel-heading
        %h3.panel-title
          = t('.title', model: model_class.model_name.human.titleize)
          .right-button
            = link_to t('.new', default: t("helpers.links.new")), new_user_registration_path, class: 'btn btn-wtc'
      = render "users/listagem"
